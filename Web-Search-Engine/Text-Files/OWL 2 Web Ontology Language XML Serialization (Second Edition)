        &lt;xsd:sequence&gt;
          &lt;xsd:group ref="owl:DataPropertyExpression"/&gt;
          &lt;xsd:group ref="owl:Individual"/&gt;
          &lt;!-- This is the source invididual  --&gt;
          &lt;xsd:element ref="owl:Literal"/&gt;
          &lt;!-- This is the target individual --&gt;
        &lt;/xsd:sequence&gt;
      &lt;/xsd:extension&gt;
