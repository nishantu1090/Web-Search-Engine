      &lt;xsd:extension base="owl:Assertion"&gt;
        &lt;xsd:sequence&gt;
          &lt;xsd:group ref="owl:Individual" minOccurs="2" maxOccurs="unbounded"/&gt;
        &lt;/xsd:sequence&gt;
      &lt;/xsd:extension&gt;
