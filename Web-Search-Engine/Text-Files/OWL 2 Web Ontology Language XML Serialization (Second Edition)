        &lt;xsd:attributeGroup ref="xml:specialAttrs"/&gt;
      &lt;/xsd:extension&gt;
    &lt;/xsd:simpleContent&gt;
  &lt;/xsd:complexType&gt;
  &lt;xsd:element name="IRI" type="owl:IRI"/&gt;

