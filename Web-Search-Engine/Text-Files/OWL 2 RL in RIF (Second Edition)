   rif:error()&nbsp;:- And(
      &nbsp;?p[rdf:type-&gt;owl:AsymmetricProperty]
      &nbsp;?x[?p-&gt;?y]
      &nbsp;?y[?p-&gt;?x]  ))

(* &lt;#prp-trp&gt; *)
