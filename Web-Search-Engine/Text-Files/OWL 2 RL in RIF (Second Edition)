  &nbsp;?p[rdfs:range-&gt;?c2]&nbsp;:- And(
      &nbsp;?p[rdfs:range-&gt;?c1]
      &nbsp;?c1[rdfs:subClassOf-&gt;?c2]  ))

(* &lt;#scm-rng2&gt; *)
