                  &lt;if&gt;
                    &lt;And&gt;
                      &lt;formula&gt;
                        &lt;Frame&gt;
                          &lt;object&gt;
                            &lt;Var&gt;s&lt;/Var&gt;
                          &lt;/object&gt;
                          &lt;slot ordered="yes"&gt;
                            &lt;Var&gt;p&lt;/Var&gt;
